{
    "name": "@eversurf/surfkeeper-provider",
    "version": "0.2.1",
    "description": "Provider interface for Surf Keeper extension of the Everscale blockchain",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/EverSurf/surf-keeper-provider.git"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "prepare": "husky install",
        "build": "tsc",
        "build:vanilla": "rollup --config rollup.config.ts --configPlugin typescript",
        "build:docs": "typedoc",
        "lint": "eslint .",
        "prettier:check": "prettier --check .",
        "prettier:fix": "prettier --write .",
        "tsc": "tsc",
        "pre-commit": "yarn tsc && yarn prettier:check"
    },
    "author": "Vit Pankin",
    "license": "GPL-3.0",
    "devDependencies": {
        "@rollup/plugin-terser": "^0.2.1",
        "@rollup/plugin-typescript": "^10.0.1",
        "@types/node": "^18.14.5",
        "@typescript-eslint/eslint-plugin": "^5.31.0",
        "@typescript-eslint/parser": "^5.31.0",
        "eslint": "^8.21.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-tsdoc": "^0.2.17",
        "husky": "^8.0.3",
        "prettier": "^2.8.4",
        "rollup": "^3.9.0",
        "typedoc": "^0.23.10",
        "typedoc-plugin-missing-exports": "^0.23.0",
        "typescript": "^4.2.4"
    },
    "files": [
        "/dist"
    ],
    "bugs": {
        "url": "https://github.com/EverSurf/surf-keeper-provider/issues"
    },
    "homepage": "https://github.com/EverSurf/surf-keeper-provider#readme",
    "keywords": [
        "ever",
        "surf",
        "keeper",
        "surfkeeper",
        "extension",
        "everscale",
        "provider"
    ]
}
